<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="repository.kodil" name="KodiIL Repository" version="1.0" provider-name="Kodi">
    <requires>
        <import addon="xbmc.addon" version="12.0.0"/>
    </requires>
    <extension point="xbmc.addon.repository" name="KodiIL Repository">
        <dir>
		<info compressed="false">https://raw.githubusercontent.com/Kodiman112/kodisrael/refs/heads/main/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/Kodiman112/kodisrael/refs/heads/main/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/Kodiman112/kodisrael/raw/refs/heads/main/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en"></summary>
        <description lang="en"></description>
        <disclaimer lang="en"></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
        <news></news>
    </extension>
</addon>

<addon id="plugin.video.tvone" name="tvOne" version="26.1.0" provider-name="RACC">
  <requires>
    <import addon="xbmc.python" version="3.0.0"/>
    <import addon="script.module.pycryptodome"/>
    <import addon="script.module.requests"/>
    <import addon="script.module.kodi-six"/>
    <import addon="script.module.future"/>
    <import addon="script.module.routing"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <reuselanguageinvoker>true</reuselanguageinvoker>
    <summary lang="en_gb">TvTap</summary>
    <description lang="en_gb">tvOne aka TvTap[CR]Updated August 2023</description>
    <disclaimer lang="en_gb"/>
    <platform>all</platform>
    <license>GPL-3.0</license>
    <assets>
        <icon>icon.png</icon>
        <fanart>fanart.jpg</fanart>
    </assets>    
  </extension>
</addon>
</addons>
